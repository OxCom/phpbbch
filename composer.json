{
  "name": "oxcom/phpbbch",
  "type": "phpbb-extension",
  "description": "This is an extension for PHPBB 3.1 that allows to highlight the code",
  "homepage": "https://github.com/oxcom/phpbbch",
  "version": "1.0.1",
  "keywords": ["phpbb", "extension", "highlight"],
  "license": "MIT",
  "authors": [
    {
      "name": "OxCom",
      "email": "lancer.oxcom@gmail.com",
      "homepage": "https://github.com/oxcom/",
      "role": "Magic Wand"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.4.0"
  },
  "autoload-dev": {
    "psr-4": {
      "oxcom\\phpbbch\\": ""
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.8"
  },
  "extra": {
    "display-name": "PHPBB Code Highlight",
    "soft-require": {
      "phpbb/phpbb": "~3.1"
    }
  }
}
